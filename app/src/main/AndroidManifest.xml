<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.joaos.virtualhelper">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icone_logo_nova"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ObraDetalhadaActivity" />
        <activity android:name=".activity.edit.ObraDetalhadaEditActivity" />
        <activity android:name=".activity.edit.ContainerEditActivity" />
        <activity android:name=".activity.RecomendarObra" />
        <activity android:name=".activity.edit.TagEditActivity" />
        <activity android:name=".activity.ResultadoPesquisaActivity" />
        <activity android:name=".activity.ResultadoScannerActivity"></activity>
    </application>

</manifest>