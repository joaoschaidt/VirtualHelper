            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity android:name=".activity.MainActivity" />
        <activity android:name=".activity.ObraDetalhadaActivity" />
        <activity android:name=".activity.ObraDetalhadaEditActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ContainerEditActivity"></activity>
        <activity android:name=".activity.RecomendarObra"></activity>
    </application>

</manifest>